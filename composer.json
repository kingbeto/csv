{
  "name": "kingbeto/csv",
  "description": "Laravel 12 package for writing and reading CSV files. It's a respawn of wilgucki/csv",
  "keywords": ["Laravel", "csv"],
  "license": "MIT",
  "authors": [
    {
      "name": "Maciej Wilgucki",
      "email": "mwilgucki@gmail.com"
    },
    {
      "name": "Alberto Pardo Saleme",
      "email": "apardo@engeni.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "kingbeto/php-csv": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Wilgucki\\Csv\\": "src",
      "Wilgucki\\Csv\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wilgucki\\Csv\\CsvServiceProvider"
      ],
      "aliases": {
        "CsvReader": "Wilgucki\\Csv\\Facades\\Reader",
        "CsvWriter": "Wilgucki\\Csv\\Facades\\Writer"
      }
    }
  }
}
